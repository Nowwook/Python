<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="UTF-8">
    <link rel="stylesheet" type="text/css" href="/css/style.css">
</head>
<body>
    <table border="2">
        <tr>
            <th>ID</th>
            <th>NAME</th>
            <th>product</th>
            <th>buytime</th>
            <% for(var i=0; i<data.length; i++){ %>
                <tr>
                    <td><%= data[i].id %> </td>
                    <td><%= data[i].name %> </td>
                    <td><%= data[i].product %> </td>
                    <td><%= data[i].buytime %> </td>
                    <p></p>
                </tr>
            <% } %>
        </tr>
    </table>
</body>
</html>
